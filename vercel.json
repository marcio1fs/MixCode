{
  "version": 2,
  "name": "mixcode-ai-assisted-ide",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "apps/ai-services/**/*.py": {
      "runtime": "python3.11"
    }
  }
} 